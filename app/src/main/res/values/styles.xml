<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="tabbar_label">
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textSize">12sp</item>
        <item name="android:lineHeight">16sp</item>
        <item name="android:fontWeight">500</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="roboto_top">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineHeight">19sp</item>
        <item name="android:fontWeight">500</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="roboto_title">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineHeight">18.75sp</item>
        <item name="android:fontWeight">700</item>
        <item name="android:textColor">@color/black_light</item>
    </style>

    <style name="sf_ui_category_selected">
        <item name="android:fontFamily">@font/sf_ui_display_medium</item>
        <item name="android:textSize">13sp</item>
        <item name="android:lineHeight">15.5sp</item>
        <item name="android:fontWeight">600</item>
        <item name="android:textColor">@color/figma_B1</item>
    </style>

    <style name="sf_ui_category">
        <item name="android:fontFamily">@font/sf_ui_display_medium</item>
        <item name="android:textSize">13sp</item>
        <item name="android:lineHeight">15.5sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:textColor">@color/grey_text</item>
    </style>

    <style name="sf_ui_price" parent="sf_ui_category">
        <item name="android:textColor">@color/figma_B1</item>
    </style>
</resources>